    <Authors>pardahlman;enrique-avalon</Authors>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>ZyRabbit.Operations.StateMachine</AssemblyName>
    <PackageId>ZyRabbit.Operations.StateMachine</PackageId>
    <PackageTags>rabbitmq;zyrabbit;operation;statemachine;stateless</PackageTags>
    <PackageIconUrl>http://pardahlman.se/raw/icon.png</PackageIconUrl>
